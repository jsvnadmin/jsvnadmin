<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         version="3.0">
         
	<display-name>svnadmin</display-name>
	<context-param>
		<param-name>contextConfigLocation</param-name>
		<param-value>/WEB-INF/applicationContext.xml</param-value>
	</context-param>
	<filter>
		<filter-name>encodingFilter</filter-name>
		<filter-class>org.springframework.web.filter.CharacterEncodingFilter</filter-class>
		<init-param>
			<param-name>encoding</param-name>
			<param-value>UTF-8</param-value>
		</init-param>
		<init-param>
			<param-name>forceEncoding</param-name>
			<param-value>true</param-value>
		</init-param>
	</filter>
	<filter-mapping>
		<filter-name>encodingFilter</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>
	<listener>
		<listener-class>org.springframework.web.context.ContextLoaderListener</listener-class>
	</listener>

	<session-config>
		<session-timeout>60</session-timeout>
	</session-config>
	<welcome-file-list>
		<welcome-file>login.jsp</welcome-file>
	</welcome-file-list>
	<error-page>
		<error-code>500</error-code>
		<location>/error.jsp</location>
	</error-page>
	<servlet>
		<servlet-name>ajax</servlet-name>
		<servlet-class>org.svnadmin.servlet.AjaxServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ajax</servlet-name>
		<url-pattern>*.ajax</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>org.svnadmin.servlet.LoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	<servlet>
		<display-name>usr</display-name>
		<servlet-name>usr</servlet-name>
		<servlet-class>org.svnadmin.servlet.UsrServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>usr</servlet-name>
		<url-pattern>/usr</url-pattern>
	</servlet-mapping>
	<servlet>
		<display-name>pj</display-name>
		<servlet-name>pj</servlet-name>
		<servlet-class>org.svnadmin.servlet.PjServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>pj</servlet-name>
		<url-pattern>/pj</url-pattern>
	</servlet-mapping>
	<servlet>
		<display-name>pjusr</display-name>
		<servlet-name>pjusr</servlet-name>
		<servlet-class>org.svnadmin.servlet.PjUsrServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>pjusr</servlet-name>
		<url-pattern>/pjusr</url-pattern>
	</servlet-mapping>
	<servlet>
		<display-name>pjgr</display-name>
		<servlet-name>pjgr</servlet-name>
		<servlet-class>org.svnadmin.servlet.PjGrServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>pjgr</servlet-name>
		<url-pattern>/pjgr</url-pattern>
	</servlet-mapping>
	<servlet>
		<display-name>pjgrusr</display-name>
		<servlet-name>pjgrusr</servlet-name>
		<servlet-class>org.svnadmin.servlet.PjGrUsrServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>pjgrusr</servlet-name>
		<url-pattern>/pjgrusr</url-pattern>
	</servlet-mapping>
	<servlet>
		<display-name>pjauth</display-name>
		<servlet-name>pjauth</servlet-name>
		<servlet-class>org.svnadmin.servlet.PjAuthServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>pjauth</servlet-name>
		<url-pattern>/pjauth</url-pattern>
	</servlet-mapping>
	<servlet>
		<display-name>changelang</display-name>
		<servlet-name>changelang</servlet-name>
		<servlet-class>org.svnadmin.servlet.ChangeLangServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>changelang</servlet-name>
		<url-pattern>/changelang</url-pattern>
	</servlet-mapping>
	<servlet>
		<display-name>i18n</display-name>
		<servlet-name>i18n</servlet-name>
		<servlet-class>org.svnadmin.servlet.I18nServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>i18n</servlet-name>
		<url-pattern>/i18n</url-pattern>
	</servlet-mapping>
	<servlet>
		<display-name>i18nadd</display-name>
		<servlet-name>i18nadd</servlet-name>
		<servlet-class>org.svnadmin.servlet.I18nAddServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>i18nadd</servlet-name>
		<url-pattern>/i18nadd</url-pattern>
	</servlet-mapping>
	<servlet>
		<display-name>rep</display-name>
		<servlet-name>rep</servlet-name>
		<servlet-class>org.svnadmin.servlet.RepositoryServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>rep</servlet-name>
		<url-pattern>/rep</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>H2</servlet-name>
		<servlet-class>org.h2.server.web.WebServlet</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet-mapping>
		<servlet-name>H2</servlet-name>
		<url-pattern>/h2/*</url-pattern>
	</servlet-mapping>
</web-app>